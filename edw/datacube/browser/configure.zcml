<configure
  xmlns="http://namespaces.zope.org/zope"
  xmlns:browser="http://namespaces.zope.org/browser"
  i18n_domain="scoreboard.visualization">

  <browser:resourceDirectory
    name="edw.datacube.css"
    directory="css/"
    />

  <browser:resourceDirectory
    name="edw.datacube.js"
    directory="js/"
    />

  <browser:page
    name="datacube-view.html"
    for="..interfaces.IDataCube"
    permission="zope2.View"
    class=".datacube.DataCubeView"
    template="templates/view.pt"
    />

  <browser:page
    name="relations"
    for="..interfaces.IDataCube"
    permission="zope2.View"
    class=".datacube.DataCubeView"
    attribute="relations"
    />

  <browser:page
    name="datasets"
    for="..interfaces.IDataCube"
    permission="zope2.View"
    class=".query.AjaxDataView"
    attribute="datasets"
    />

  <browser:page
    name="data-rdf"
    for="..interfaces.IDataCube"
    permission="zope2.View"
    class=".query.AjaxDataView"
    attribute="dump_rdf"
    />

  <browser:page
    name="tsv"
    for="..interfaces.IDataCube"
    permission="zope2.View"
    class=".query.AjaxDataView"
    attribute="download_tsv"
    />

  <browser:page
    name="csv"
    for="..interfaces.IDataCube"
    permission="zope2.View"
    class=".query.AjaxDataView"
    attribute="download_csv"
    />

  <browser:page
    name="dataset_metadata"
    for="..interfaces.IDataCube"
    permission="zope2.View"
    class=".query.AjaxDataView"
    attribute="dataset_metadata"
    />

  <browser:page
    name="dataset_details"
    for="..interfaces.IDataCube"
    permission="zope2.View"
    class=".query.AjaxDataView"
    attribute="dataset_details"
    />

  <browser:page
    name="dimensions"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="get_dimensions"
    permission="zope2.View"
    />

  <browser:page
    name="dimension_options"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="dimension_options"
    permission="zope2.View"
    />

  <browser:page
    name="my_breakdowns"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="my_breakdowns"
    permission="zope2.View"
    />

  <browser:page
    name="my_indicators"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="my_indicators"
    permission="zope2.View"
    />

  <browser:page
    name="my_unitmeasures"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="my_unitmeasures"
    permission="zope2.View"
    />

  <browser:page
    name="revision"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="revision"
    permission="zope2.View"
    />

  <browser:page
    name="dimension_labels"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="dimension_labels"
    permission="zope2.View"
    />

  <browser:page
    name="dimension_codelist"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="dimension_codelist"
    permission="zope2.View"
    />

  <browser:page
    name="notations"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="notations"
    permission="zope2.View"
    />

  <browser:page
    name="dimension_options_xy"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="dimension_options_xy"
    permission="zope2.View"
    />

  <browser:page
    name="dimension_options_xyz"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="dimension_options_xyz"
    permission="zope2.View"
    />

  <browser:page
    name="dimension_options_cp"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="dimension_options_cp"
    permission="zope2.View"
    />

  <browser:page
    name="dimension_value_metadata"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="dimension_value_metadata"
    permission="zope2.View"
    />

  <browser:page
    name="datapoints"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="datapoints"
    permission="zope2.View"
    />

  <browser:page
    name="datapoints_xy"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="datapoints_xy"
    permission="zope2.View"
    />

  <browser:page
    name="datapoints_xyz"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="datapoints_xyz"
    permission="zope2.View"
    />

  <browser:page
    name="datapoints_cp"
    for="..interfaces.IDataCube"
    class=".query.AjaxDataView"
    attribute="datapoints_cp"
    permission="zope2.View"
    />

  <browser:page
    name="export.csv"
    for="..interfaces.IDataCube"
    class=".export.ExportCSV"
    attribute="export"
    permission="zope2.View"
    />

  <browser:page
    name="export.rdf"
    for="..interfaces.IDataCube"
    class=".export.ExportRDF"
    attribute="export"
    permission="zope2.View"
    />

  <browser:page
    name="export.svg"
    for="..interfaces.IDataCube"
    class=".export.ExportSvg"
    attribute="export"
    permission="zope2.View"
    />

  <browser:page
    name="svg2png"
    for="..interfaces.IDataCube"
    class=".export.SvgToPng"
    attribute="convert"
    permission="zope2.View"
    />

  <browser:page
    name="codelists"
    for="..interfaces.IDataCube"
    permission="zope2.View"
    class=".query.AjaxDataView"
    attribute="download_codelists"
    />

  <browser:page
    name="structure"
    for="..interfaces.IDataCube"
    permission="zope2.View"
    class=".query.AjaxDataView"
    attribute="download_structure"
    />

  <browser:page
    name="get_dataset_metadata"
    for="..interfaces.IDataCube"
    permission="zope2.View"
    class=".query.AjaxDataView"
    attribute="download_dataset_metadata"
    />

</configure>
